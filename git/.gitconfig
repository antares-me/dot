[user]
	email = antares@antares.me
	name = antares-me (Anton Salnikov)
[github]
	user = antares-me
[push]
	default = current
[filter "lfs"]
	smudge = git-lfs smudge %f
	required = true
	clean = git-lfs clean %f
[core]
	editor = vim
	pager = less -SR
[merge]
        conflictstyle = diff3
[color "branch"]
        current = yellow reverse
        local = yellow
        remote = green
[color "diff"]
        meta = yellow bold
        frag = magenta bold
        old = red bold
        new = green bold
[color "status"]
        added = yellow
        changed = green
        untracked = cyan
[alias]
        st = status
        ci = commit
        br = branch
        co = checkout
        df = diff
        lg = log --oneline --graph --decorate
